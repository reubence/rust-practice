fn main() {
	let v = vec![4,8,14];
	let k = 8;
	let result = filter_lt_k(&v, k);
	println!("{:?}", result);
}

pub fn filter_lt_k(v: &Vec<i32>, k: i32) -> Vec<i32> {
    let mut vec = vec![];

    if v.len() == 0 {
        return vec;
    }

    for i in 0..v.len() {
        if v[i] >= k {
            vec.push(v[i]);
        }   
    }

    vec
}